<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
    namespace:命名空间，相当于包名
    #{}：相当于占位符
-->
<mapper namespace="com.zf.mapper.ImagesMapper">
    <select id="getAllImage" resultType="com.zf.pojo.Images">
        select * from images;
    </select>
</mapper>
